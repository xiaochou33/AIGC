{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "Node",
        "useDefineForClassFields": false,
        "strict": true,
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "lib": ["ES2022", "DOM"],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "useUnknownInCatchVariables": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "paths": {}
    }
}
